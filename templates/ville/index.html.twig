{% extends 'base.html.twig' %}

{% block title %}Ajouter une nouvelle ville{% endblock %}

{% block body %}
    <div class="f-form">
        <div class="f-head">
            <h1>Ajouter une nouvelle ville</h1>
        </div>

        {{ form_start(form) }}
        <div class="f-button-container">
            <a class="f-button f-button-secondary" href="{{ path('app_lieu_add') }}">Annuler</a>
            <button class="f-button" type="submit">Enregistrer</button>
        </div>
        {{ form_end(form) }}

    </div>
{% endblock %}
