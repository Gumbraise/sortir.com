<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('images/static/favicon.ico') }}"/>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
{% for type, messages in app.flashes %}
    {% for message in messages %}
        <div
                {{ stimulus_controller('notyf',{
                    type: type,
                    text: message
                }) }}
        ></div>
    {% endfor %}
{% endfor %}
{{ include('_includes/navbar.html.twig') }}
{% block body %}{% endblock %}
</body>
</html>
